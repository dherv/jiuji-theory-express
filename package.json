{
  "name": "jiuji-theory-server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "yarn run build-ts && yarn run lint",
    "build-ts": "tsc",
    "debug": "yarn run build && yarn run watch-debug",
    "serve": "node dist/index.js",
    "serve-debug": "nodemon --inspect dist/index.js",
    "serve:run": "yarn run serve",
    "prestart": "tsc",
    "start": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"yarn run watch-ts\" \"yarn run watch-node\"",
    "start:debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\"  \"yarn run watch-ts\" \"yarn run serve-debug\"",
    "watch-node": "nodemon dist/src/index.js",
    "watch-ts": "tsc -w",
    "prettier:check": "prettier --check src/**/*.ts",
    "prettier:write": "prettier --write src/**/*.ts",
    "lint": "tsc --noEmit && yarn eslint . --ext .js,.jsx,.ts,.tsx --quiet --fix",
    "test": "jest --watchAll --passWithNoTests",
    "test:ci": "jest --passWithNoTests",
    "cypress": "cypress open"
  },
  "dependencies": {
    "@prisma/cli": "^2.4.0",
    "@prisma/client": "^2.4.0",
    "@sentry/node": "5.20.1",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.7",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.7",
    "@types/helmet": "^0.0.47",
    "@types/jest": "^26.0.9",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.0.27",
    "@types/webpack": "^4.41.21",
    "@types/winston": "^2.4.4",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.23.3",
    "morgan": "^1.10.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "concurrently": "^5.2.0",
    "cypress": "^4.11.0",
    "eslint": "7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.2.2",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.1.4",
    "ts-loader": "^8.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-node-externals": "^2.5.0"
  }
}
